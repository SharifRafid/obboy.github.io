{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tailwindcss -i ./src/tailwind.src.css -o ./src/tailwind.css && rollup -c",
    "dev": "tailwindcss -i ./src/tailwind.src.css -o ./src/tailwind.css && concurrently \"tailwindcss -i ./src/tailwind.src.css -o ./src/tailwind.css -w\" \"rollup -c -w\"",
    "start": "sirv public --no-clear",
    "tailwind-dev": "tailwindcss -i ./src/tailwind.src.css -o ./src/tailwind.css -w",
    "svelte-dev": "rollup -c -w"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "autoprefixer": "^10.3.7",
    "concurrently": "^6.3.0",
    "postcss": "^8.3.11",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "tailwindcss": "^2.2.17"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  }
}
